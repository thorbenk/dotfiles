[
  {
    "context": "Dock",
    "bindings": {
      "ctrl-w h": ["workspace::ActivatePaneInDirection", "Left"],
      "ctrl-w l": ["workspace::ActivatePaneInDirection", "Right"],
      "ctrl-w k": ["workspace::ActivatePaneInDirection", "Up"],
      "ctrl-w j": ["workspace::ActivatePaneInDirection", "Down"]
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "f7": [
        "task::Spawn",
        {
          "task_name": "build"
        }
      ],
      "f6": [
        "task::Spawn",
        {
          "task_name": "run"
        }
      ],
      "f4": "editor::SwitchSourceHeader",
      "alt-e": "workspace::ToggleLeftDock"
    }
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-b": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "VimControl && !menu",
    "bindings": {
      "space f": "file_finder::Toggle",
      "space s": "project_symbols::Toggle",
      "space c": "pane::CloseInactiveItems",
      "space z": "workspace::ToggleZoom",
      "space o o ": "outline_panel::ToggleFocus",
      "ctrl-b": "terminal_panel::ToggleFocus",
      "space g g": [
        "workspace::SendKeystrokes",
        "shift esc : new center terminal enter lazygit enter"
      ],
      "] b": "pane::ActivateNextItem",
      "[ b": "pane::ActivatePrevItem"
    }
  }
]
